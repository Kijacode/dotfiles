   "  ${5:<Button\n    onClick={() => {\n      let toastKey;\n      const msg =\n        \"Use toaster.info(), toaster.positive(), toaster.warning(), or toaster.negative()\";\n      const ok = (\n        <Block\n          marginTop=\"15px\"\n          display=\"flex\"\n          justifyContent=\"center\"\n        >\n          <Button\n            size={SIZE.compact\\}\n            onClick={() => toaster.clear(toastKey)\\}\n          >\n            Ok\n          </Button>\n        </Block>\n      );\n      const showMore = (\n        <Block\n          marginTop=\"15px\"\n          display=\"flex\"\n          justifyContent=\"left\"\n        >\n          <Button\n            size={SIZE.compact\\}\n            onClick={() =>\n              toaster.update(toastKey, {\n                children: (\n                  <>\n                    {msg\\} to show different notification\n                    type. {ok\\}\n                  </>\n                )\n              \\})\n            \\}\n          >\n            Show more\n          </Button>\n        </Block>\n      );\n      toastKey = toaster.info(\n        <>\n          {msg\\}\n          {showMore\\}\n        </>,\n        {\n          onClose: () => console.log(\"Toast closed.\"),\n          overrides: {\n            InnerContainer: { style: { width: \"100%\" \\} \\}\n          \\}\n        \\}\n      );\n    \\}\\}\n  >\n    Show notification\n  </Button>}",
