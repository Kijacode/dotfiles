{
	"name": "preview-vscode",
	"displayName": "Preview",
	"description": "A Markdown, ReStructured Text, HTML, Jade, Image, CSS, Mermaid previewer",
	"version": "1.3.0",
	"publisher": "myh",
	"author": "Meng-Yuan Huang",
	"license": "MIT",
	"readme": "README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/MrMYHuang/preview-vscode"
	},
	"galleryBanner": {
		"color": "#78d6f0",
		"theme": "light"
	},
	"bugs": "https://github.com/MrMYHuang/preview-vscode/issues",
	"homepage": "https://github.com/MrMYHuang/preview-vscode/blob/master/README.md",
	"icon": "images/icon.png",
	"main": "./out/src/extension",
	"engines": {
		"vscode": "^0.10.8"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.preview",
		"onCommand:extension.previewToSide"
	],
	"contributes": {
		"languages": [
			{
				"id": "mermaid",
				"aliases": [
					"Mermaid",
					"Mermaid Flowchart"
				],
				"extensions": [
					".mmd",
					".mermaid"
				]
			},
			{
				"id": "rst",
				"aliases": [
					"RST",
					"ReStructured Text"
				],
				"extensions": [
					".rst",
					".rest",
					".hrst"
				]
			}
		],
		"keybindings": [
			{
				"command": "extension.preview",
				"key": "ctrl+shift+v",
				"mac": "cmd+shift+v"
			},
			{
				"command": "extension.preview",
				"key": "ctrl+shift+v",
				"mac": "ctrl+shift+v"
			},
			{
				"command": "extension.previewToSide",
				"key": "ctrl+k v",
				"mac": "cmd+k v"
			},
			{
				"command": "extension.previewToSide",
				"key": "ctrl+k v",
				"mac": "ctrl+k v"
			}
		],
		"commands": [
			{
				"command": "extension.preview",
				"title": "Toggle Preview",
				"category": "Preview"
			},
			{
				"command": "extension.previewToSide",
				"title": "Open Preview to the Side",
				"category": "Preview"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"file-url": "^2.0.2",
		"markdown": "^0.5.0",
		"mermaid": "^7.0.0",
		"rst2mdown": "^0.1.0"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.32",
		"@types/node": "^7.0.8",
		"mocha": "^3.2.0",
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"tslint": "^4.5.1",
		"jslint": "^0.10.3"
	},
	"__metadata": {
		"id": "3be06c51-6e67-48a6-820d-127c38be77db",
		"publisherId": "4b68b6bf-ffd3-4021-9608-6d8ffd4239c9",
		"publisherDisplayName": "Meng-Yuan Huang"
	}
}