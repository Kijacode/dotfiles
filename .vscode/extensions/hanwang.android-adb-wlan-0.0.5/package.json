{
	"name": "android-adb-wlan",
	"displayName": "Android ADB WLAN",
	"description": "connect android with wlan",
	"version": "0.0.5",
	"preview": true,
	"icon": "android.png",
	"license": "MIT",
	"publisher": "HanWang",
	"repository": {
		"type": "git",
		"url": "https://github.com/sewerganger/android-adb-wlan"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Debuggers",
		"Other"
	],
	"activationEvents": [
		"onCommand:android.adb.connect",
		"onCommand:android.adb.restart"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "android.adb.connect",
				"title": "Android adb wlan connect",
				"icon": {
					"dark": "assets/adb.svg",
					"light": "assets/adb.svg"
				}
			},
			{
				"command": "android.adb.restart",
				"title": "Android adb restart"
			}
		],
		"keybindings": [
			{
				"command": "android.adb.connect",
				"key": "ctrl+alt+]"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "android.adb.connect",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc --watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/ip": "^1.1.0",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^2.18.5",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.9.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"delay": "^4.3.0",
		"execa": "^4.0.0",
		"ip": "^1.1.5",
		"open": "^7.0.2"
	},
	"__metadata": {
		"id": "9d04e8cc-77cf-424c-804a-b2cf14c9284c",
		"publisherId": "3dfd562d-6c8c-46b6-83b7-2a2a30705abf",
		"publisherDisplayName": "HanWang"
	}
}